/** * @author gerd */class com.adgamewonderland.ricola.sudoku.ui.GameControllerUI extends com.adgamewonderland.aldi.sudoku.ui.GameControllerUI {		private var stop_btn:Button;		public function GameControllerUI() {		super();		// stop		stop_btn.onRelease = function() {			this._parent.onStop();		};	}		public function onStop():Void	{		// spiel beenden		game_mc.stopGame(true);	}		public function onSound():Void	{		_root.sound_mc.toggleSound();	}}
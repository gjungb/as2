import com.adgamewonderland.ricola.sudoku.ui.BoexliAniUI;/** * @author gerd */class com.adgamewonderland.ricola.sudoku.ui.SupplyUI extends MovieClip {		private var boexliani:Array;		public function SupplyUI() {		// registrierte boexli animationen		this.boexliani = new Array();	}		public function init():Void	{		// schleife ueber boexliani		for (var i : String in boexliani) {			BoexliAniUI(boexliani[i]).init();		}	}		public function setEnabled(bool:Boolean ):Void	{		// schleife ueber boexliani		for (var i : String in boexliani) {			BoexliAniUI(boexliani[i]).enabled = bool;		}		}		public function registerBoexli(mc:BoexliAniUI ):Void	{		// speichern		this.boexliani.push(mc);	}		public function onShowBoexliAni(mc:BoexliAniUI ):Void	{		// aktuelle boexliani		var b:BoexliAniUI;		// alle anderen zurueck setzen		for (var i : String in boexliani) {			// aktuelle			b = boexliani[i];			// nur andere als caller			if (b != mc) {				b.hideAni();			}		}	}	}
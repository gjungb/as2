import com.adgamewonderland.eplus.base.tarifberater.automat.AbstractZustand;import com.adgamewonderland.eplus.base.tarifberater.automat.TarifberaterAutomat;import com.adgamewonderland.eplus.base.tarifberater.beans.Antwort;import com.adgamewonderland.eplus.base.tarifberater.beans.BeraterTarif;import com.adgamewonderland.eplus.base.tarifberater.beans.BeraterOnline;import com.adgamewonderland.eplus.base.tarifberater.beans.Frage;import com.adgamewonderland.eplus.base.tarifberater.interfaces.IProdukt;import com.adgamewonderland.eplus.base.tarifberater.interfaces.IZustand;import com.adgamewonderland.eplus.base.tarifberater.beans.BeraterTarifOption;import com.adgamewonderland.eplus.base.tarifberater.beans.AbstractProdukt;class com.adgamewonderland.eplus.base.tarifberater.automat.InternetFlatrateZustand extends AbstractZustand {	public function InternetFlatrateZustand(aAutomat : TarifberaterAutomat ) {		super(aAutomat);		// id		this.id = "InternetFlatrateZustand";		// fortschritt		this.fortschritt = 7;		// frage		this.frage = new Frage("Auswahl SIM-Kartenanzahl");		this.frage.addAntwort(new Antwort("AntwortUI", "BeraterOnline", BeraterOnline.ONLINE_1SIM, "Ich möchte nur eine \neinzige SIM-Karte ", "5.3.1"));		this.frage.addAntwort(new Antwort("AntwortUI", "BeraterOnline", BeraterOnline.ONLINE_2SIM, "Ich möchte eine \nseparate SIM-Karte für \nmeinen Laptop", "5.3.2"));		this.frage.addAntwort(new Antwort("AntwortUI", "BeraterOnline", BeraterOnline.ONLINE_MICROSIM, "Ich möchte eine \nseparate Micro-SIM-Karte für meinen Tablet-PC", "5.3.3"));	}	public function produktWaehlen(aProdukt : IProdukt) : Void {		// gewaehlte tarifoption		var tarifoption : IProdukt = BeraterOnline(aProdukt);		// naechster zustand		var zustand : IZustand = this.automat.getFehlerZustand();		// je nach produkt muss anderer zustand gewaehlt werden		switch (tarifoption.getId()) {			// als option zum vorhandenen tarif			case BeraterOnline.ONLINE_1SIM :				// mit internet laptop flat				tarifoption = AbstractProdukt.getProdukt(BeraterTarifOption.INTERNET_FLAT_XL);				// step				tarifoption.setStep(aProdukt.getStep());				// sichtbar machen				tarifoption.setVisible(true);				// tarifoption hinzufuegen				this.automat.getBerater().addProdukt(tarifoption);				// fertig				zustand = this.automat.getKonfiguration().getEnde();							break;							// als eigener tarif			case BeraterOnline.ONLINE_2SIM :				// neuer tarif 				var tarif : IProdukt = AbstractProdukt.getProdukt(BeraterTarif.MEINBASE);				// step				tarif.setStep(aProdukt.getStep());				// sichtbar machen				tarif.setVisible(true);				// tarif hinzufuegen				this.automat.getBerater().addProdukt(tarif);				// mit internet laptop flat				tarifoption = AbstractProdukt.getProdukt(BeraterTarifOption.INTERNET_FLAT_XL);				// step				tarifoption.setStep(aProdukt.getStep());				// sichtbar machen				tarifoption.setVisible(true);				// tarifoption hinzufuegen				this.automat.getBerater().addProdukt(tarifoption);				// fertig				zustand = this.automat.getKonfiguration().getEnde();								break;							// als eigener tarif mit micro sim			case BeraterOnline.ONLINE_MICROSIM :				// neuer tarif 				var tarif : IProdukt = AbstractProdukt.getProdukt(BeraterTarif.MEINBASE);				// step				tarif.setStep(aProdukt.getStep());				// sichtbar machen				tarif.setVisible(true);				// als micro sim				tarif.setSapnummer(BeraterTarif.SAPNUMMER_MICRO);				// tarif hinzufuegen				this.automat.getBerater().addProdukt(tarif);				// mit internet laptop flat				tarifoption = AbstractProdukt.getProdukt(BeraterTarifOption.INTERNET_FLAT_XL);				// step				tarifoption.setStep(aProdukt.getStep());				// sichtbar machen				tarifoption.setVisible(true);				// tarifoption hinzufuegen				this.automat.getBerater().addProdukt(tarifoption);				// fertig				zustand = this.automat.getKonfiguration().getEnde();								break;		}		// naechster zustand		this.automat.setZustand(zustand);	}	public function toString() : String {		return "com.adgamewonderland.eplus.base.tarifberater.automat.InternetFlatrateZustand";	}}
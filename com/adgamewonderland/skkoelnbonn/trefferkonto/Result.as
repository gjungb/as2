umber ):Void	{		// trefferbonus gesamt		myBonusTotal = num;	}		public function get bonustotal():Number	{		//  trefferbonus gesamt		return (myBonusTotal);	}		public function set revenue(num:Number ):Void	{		// ertrag		myRevenue = num;	}		public function get revenue():Number	{		//  ertrag		return (myRevenue);	}	} /* end class Result */